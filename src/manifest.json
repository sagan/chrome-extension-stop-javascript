{
    "name": "Stop Script",
    "description": "Stop javascript execution in current page",
    "version": "1.0.0",
    "author": "Jacques De SAGAN",
    "manifest_version": 2,
    "permissions": ["tabs", "cookies", "webRequest", "contextMenus", "webRequestBlocking", "*://*/*"],
    "icons": {
        "16": "icons/cross_16x16.png",
        "48": "icons/cross_48x48.png",
        "64": "icons/cross_64x64.png",
        "128": "icons/cross_128x128.png"
    },
    "content_scripts": [
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Stop Script"
    }
}